<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="283.0" prefWidth="407.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PhotosView.LoginController">
   <children>
      <Button fx:id="submit" layoutX="106.0" layoutY="139.0" mnemonicParsing="false" onAction="#submitButton" text="Submit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="username" layoutX="106.0" layoutY="91.0" />
      <Label layoutX="22.0" layoutY="95.0" text="Username:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="56.0" layoutY="14.0" prefHeight="80.0" prefWidth="314.0" text="Welcome to Photos! Please Login:">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="clear" layoutX="205.0" layoutY="139.0" mnemonicParsing="false" onAction="#clearButton" prefHeight="28.0" prefWidth="62.0" text="Clear">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="288.0" layoutY="216.0" mnemonicParsing="false" onAction="#exitButton" prefHeight="28.0" prefWidth="107.0" text="Exit Photos">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
